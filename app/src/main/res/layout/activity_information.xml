<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.charles.workingplace.Vue.Information">

    <TextView
        android:id="@+id/infoNom"
        android:layout_width="105dp"
        android:layout_height="42dp"
        app:layout_constraintBottom_toTopOf="@+id/infoAge"
        app:layout_constraintEnd_toStartOf="@+id/infoPrenom"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/infoPrenom"
        android:layout_width="93dp"
        android:layout_height="39dp"
        app:layout_constraintBottom_toTopOf="@+id/infoEmail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/infoNom"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.269" />

    <TextView
        android:id="@+id/infoAge"
        android:layout_width="69dp"
        android:layout_height="43dp"
        app:layout_constraintBottom_toTopOf="@+id/infoEmail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.196"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoNom" />

    <TextView
        android:id="@+id/infoLieu"
        android:layout_width="338dp"
        android:layout_height="51dp"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/modifierMdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoEmail" />

    <TextView
        android:id="@+id/infoEmail"
        android:layout_width="304dp"
        android:layout_height="47dp"
        app:layout_constraintBottom_toTopOf="@+id/infoLieu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoAge" />

    <Button
        android:id="@+id/modifierMdp"
        android:layout_width="wrap_content"
        android:layout_height="43dp"
        android:text="Modifier mot de passe"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoLieu" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="53dp"
        android:layout_height="24dp"
        android:text="Nom"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/infoNom"
        app:layout_constraintEnd_toStartOf="@+id/infoPrenom"
        app:layout_constraintHorizontal_bias="0.311"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="68dp"
        android:layout_height="28dp"
        android:text="Prénom"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/infoPrenom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.585"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.941" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="33dp"
        android:layout_height="25dp"
        android:text="Age"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/infoAge"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.172"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoNom"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="55dp"
        android:layout_height="29dp"
        android:text="E-mail"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/infoEmail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoAge"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/InfoLieuReserver"
        android:layout_width="wrap_content"
        android:layout_height="29dp"
        android:text="Dernier lieu reservé"
        android:textSize="24sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/infoLieu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.098"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoEmail"
        app:layout_constraintVertical_bias="1.0" />
</android.support.constraint.ConstraintLayout>
